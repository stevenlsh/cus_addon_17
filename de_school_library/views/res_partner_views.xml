<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="view_partner_form_inherit_library" model="ir.ui.view">
            <field name="name">view.partner.form.inherit.library</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form" />
            <field name="arch" type="xml">
                <xpath expr="//group[1]/group[2]" position='inside'>
                    <field name="is_publisher" invisible="1"/>
                    <field name="is_author" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>